{
  "name": "roam-web",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently \"bun run dev:server\" \"bun run dev:client\"",
    "dev:server": "bun --watch server.ts",
    "dev:client": "vite",
    "start": "bun server.ts",
    "build:css": "tailwindcss -i input.css -o static/styles.css",
    "build:tailwind": "tailwindcss -i client/src/index.css -o client/src/styles-built.css --watch",
    "build:server": "bun build server.ts --outdir ./dist",
    "build:client": "vite build",
    "build": "bun run build:client && bun run build:server",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-onboarding": "^9.1.3",
    "@storybook/addon-vitest": "^9.1.3",
    "@storybook/react-vite": "^9.1.3",
    "@types/bun": "latest",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "postcss": "^8.5.6",
    "storybook": "^9.1.3",
    "tailwindcss": "^3.4.0",
    "vite": "^7.1.3",
    "vitest": "^3.2.4",
    "@vitest/browser": "^3.2.4",
    "playwright": "^1.55.0",
    "@vitest/coverage-v8": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.85.5",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "hono": "^4.9.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "tailwind-merge": "^3.3.1"
  }
}
